<% @writings.each.with_index(1) do |writing, i| %>
  <div class="mt-5">
    <div class="text-center">
      <p><%= "#{writing.title}" %></p>
        <iframe width="560" height="315" src="<%=writing.url%>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
  </div>
<% end %>